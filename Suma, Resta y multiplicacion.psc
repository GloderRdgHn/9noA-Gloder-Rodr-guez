Algoritmo sin_titulo
	Definir N1,N2,N3,S,R,M Como Entero;
	Escribir "INGREE UN NUMERO"; 
	Leer N1;
	Escribir "INGREE UN NUMERO"; 
	Leer N2;
	Escribir "INGREE UN NUMERO"; 
	Leer N3;
	S<-N1+N2+N3;
	R<-N1-N2-N3;
	M<-N1*N2*N3;
	Escribir "LA SUMA DE LOS NUMEROS ES " , S;
	Escribir "LA RESTA DE LOS NUMEROS ES " , R;
	Escribir "LA MULTIPLICACION DE LOS NUMEROS ES " , M;
	
